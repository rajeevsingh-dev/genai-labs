# Azure OpenAI Configuration
# Copy this file to .env and fill in your actual values

# Your Azure OpenAI resource endpoint
# Example: https://your-resource-name.openai.azure.com/
AZURE_OPENAI_ENDPOINT=https://your-resource-name.openai.azure.com/

# Your Azure OpenAI API key (Key 1 or Key 2 from Azure portal)
AZURE_OPENAI_API_KEY=your_api_key_here

# Your model deployment name (the name you gave when deploying the model)
# Common examples: gpt-4, gpt-4o
AZURE_OPENAI_MODEL_NAME=gpt-4o

# Optional: API version (use latest for best features)
AZURE_OPENAI_API_VERSION=2024-02-15-preview

